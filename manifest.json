{
    "manifest_version": 3,
    "name": "Twitter用 センシティブ画像大好き君",
    "short_name": "SensitiveImageLover",
    "description": "Twitterでセンシティブ設定がされている画像を常に表示するようにします",
    "version": "1.1.1",
    "icons": {
        "128": "icon.png"
    },
    "content_scripts": [
        {
          "matches": ["https://twitter.com/*"],
          "css": ["override.css"]
        },
        {
          "matches": ["https://tweetdeck.twitter.com/*"],
          "css": ["tweetdeck_override.css"]
        }
    ]
}
